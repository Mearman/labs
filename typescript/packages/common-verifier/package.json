{
  "name": "@commontools/common-verifier",
  "author": "The Common Authors",
  "version": "0.0.1",
  "description": "",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "clean": "wireit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commontoolsinc/labs.git"
  },
  "bugs": {
    "url": "https://github.com/commontoolsinc/labs/issues"
  },
  "homepage": "https://github.com/commontoolsinc/labs#readme",
  "devDependencies": {
    "@shoelace-style/shoelace": "^2.15.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.6",
    "wireit": "^0.14.4"
  },
  "wireit": {
    "build:tsc": {
      "command": "tsc --build -f",
      "files": [
        "./src/**/*"
      ],
      "output": [
        "./public/js/**/*"
      ]
    },
    "build": {
      "dependencies": [
        "build:tsc"
      ],
      "command": "vite build"
    },
    "clean": {
      "dependencies": [],
      "command": "rm -rf ./lib ./dist ./.wireit ./node_modules"
    }
  },
  "dependencies": {
    "@types/chrome": "^0.0.268",
    "@types/serviceworker": "^0.0.87"
  }
}
